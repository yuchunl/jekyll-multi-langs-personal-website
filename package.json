{
  "name": "yuchunlin",
  "displayName": "Yu-Chun Lin Personal Website",
  "version": "3.0.0",
  "description": "Yu-Chun Lin Personal Website",
  "main": "index.js",
  "scripts": {
    "start": "npm run scaffold && concurrently -k -n \"WEBPACK,JEKYLL,BROWSER-SYNC\" -c \"bgBlue.bold,bgRed.bold,bgCyan.bold\" \"webpack -w\" \"bundle exec jekyll build --watch\" \"browser-sync start -s _site -f _site --extensions html\"",
    "serve": "http-server _site/ -p 8081",
    "eslint": "eslint _assets/js/",
    "clean": "shx rm -rf _site/",
    "sass-lint": "sass-lint -c sass-lint.yml '**/*.scss' -v -q",
    "webpack": "webpack",
    "scaffold": "npm run clean && shx mkdir -p _site/assets && shx cp -r _assets/images/. _site/assets",
    "predist": "rimraf _site/ build/",
    "dist": "npm run scaffold && webpack -p --progress && bundle exec jekyll build --config _config.yml",
    "postdist": "mkdirp build/ && cd _site/ && bestzip ../build/out.zip ."
  },
  "author": "Yu-Chun Lin",
  "dependencies": {
    "bootstrap": "4.1.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.14.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "bestzip": "^1.1.4",
    "browser-sync": "^2.18.13",
    "concurrently": "^3.5.0",
    "css-loader": "^0.28.11",
    "eslint": "^4.12.1",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-es5": "^1.1.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "postcss": "^6.0.8",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.1",
    "shx": "^0.2.2",
    "style-loader": "^0.19.0",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
